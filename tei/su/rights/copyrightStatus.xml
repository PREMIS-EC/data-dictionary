<?xml version="1.0" encoding="UTF-8"?>
<div type="elementDocumentation" xmlns="http://www.tei-c.org/ns/1.0"
    xml:id="copyrightStatus">
    <head>
        <!-- PREMIS Semantic unit 4.1.3.1 -->
        <abbr>4.1.3.1</abbr>
        <gi>copyrightStatus</gi>
    </head>
    <div type="semanticcomponents">
        <!-- PREMIS Semantic components -->
        <p>None</p>
    </div>
    <div type="definition">
        <!-- PREMIS Definition -->
        <p>A coded designation for the copyright status of the object at the time
            the Rights statement was recorded.</p>
    </div>
    <div type="rationale">
        <!-- PREMIS Rationale -->
        <p/>
    </div>
    <div type="datatype">
        <!-- PREMIS Data constraint -->
        <p>Values should be taken from a controlled vocabulary. A controlled
            vocabulary is available at: <ref
                target="http://id.loc.gov/vocabulary/preservation/copyrightStatus.html"
                >http://id.loc.gov/vocabulary/preservation/copyrightStatus.html</ref>.</p>
    </div>
    <div type="examples">
        <!-- PREMIS Examples -->
        <p>
            <list type="ordered">
                <item>copyrighted = Under copyright.</item>
                <item>publicdomain = In the public domain.</item>
                <item>unknown = Copyright status of the resource is unknown.</item>
            </list>
        </p>
    </div>
    <div type="entity">
        <p>
            <list type="simple">
                <label>Obligation</label>
                <item>Mandatory</item>
                <label>Repeatability</label>
                <item>Not epeatable</item>
            </list>
        </p>
    </div>
    <div type="creationmaintenance">
        <!-- PREMIS Creation / Maintenance -->
